@page
@model WebRazor.Pages.IndexModel
@{
	if (Model.Cart == null || Model.Cart.Count == 0)
	{
		ViewData["cartSize"] = 0;
	}
	else
	{
		ViewData["cartSize"] = Model.Cart.Count;
	}
}
<style>
	body {
		font-family: 'Arial', sans-serif;
		background-color: #f4f4f4;
		margin: 0;
		padding: 0;
	}

	#content-left{
		float: left;
		width: 20%;
		margin: 1%;
		background-color: #fff;
		padding: 20px;
		border-radius: 8px;
		box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);
	}

	#content-right {
		float: left;
		width: 78%;
		margin: 1%;
		background-color: #fff;
		padding: 20px;
		border-radius: 8px;
		box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);
		text-align: center; /* Center the content */
	}
	h3 {
		color: #333;
	}

	ul {
		list-style: none;
		padding: 0;
	}

	li {
		margin-bottom: 10px;
	}

	.content-main {
		display: flex;
		flex-wrap: wrap;
		justify-content: space-between;
	}

	.product {
		width: 30%;
		margin-bottom: 20px;
		padding: 10px;
		background-color: #fff;
		border-radius: 8px;
		box-shadow: 0 0 5px rgba(0, 0, 0, 0.1);
		transition: transform 0.3s ease-in-out;
		margin-left:70px;
		margin-right: 70px;

	}

		.product:hover {
			transform: scale(1.05);
		}

	.name a {
		color: #333;
		text-decoration: none;
	}
	.buynow{
		font-size: 40px;
	}
	.price {
		color: #f00;
		font-weight: bold;
	}

	a {
		color: #3498db;
		text-decoration: none;
	}

		a:hover {
			text-decoration: underline;
		}

	.path {
		background-color: #3498db;
		color: #fff;
		padding: 10px;
		border-radius: 4px;
		margin-bottom: 10px;
	}

	.search-form {
		margin-bottom: 20px;
		width:400px;
		
	}

	.sr-only {
		position: absolute;
		width: 1px;
		height: 1px;
		padding: 0;
		margin: -1px;
		overflow: hidden;
		clip: rect(0, 0, 0, 0);
		border: 0;
	}

	.input-group {
		display: flex;
		margin-top: 10px;
	}

	.form-control {
		flex: 1;
		padding: 10px;
		border: 1px solid #ccc;
		border-radius: 5px 0 0 5px;
	}

	.btn {
		padding: 10px 15px;
		border: 1px solid #007bff;
		border-radius: 0 5px 5px 0;
		background-color: #007bff;
		color: #fff;
		cursor: pointer;
	}

		.btn:hover {
			background-color: #0056b3;
			border-color: #0056b3;
		}
</style>

<div id="content-left">
	<h3>CATEGORY</h3>
	<ul>
		@foreach (var item in Model.Categories)
		{
			<a asp-route-id="@item.CategoryId">
			<li>@item.CategoryName</li>
			</a>
		}
		<li>@ViewData["size"]</li>
	</ul>
</div>
<div id="content-right">
	<form method="get" style="margin-left:260px" asp-page-handler="Search" class="search-form">
		<label for="searchQuery" class="sr-only">Search by Name:</label>
		<div class="input-group">
			<input type="text" id="searchQuery" name="SearchQuery" placeholder="Search by Name" class="form-control" />
			<div class="input-group-append" style="margin-left: 10px">
				<button type="submit" class="btn btn-primary">Search</button>
			</div>
		</div>
	</form>
	@if (Model.Products.Count > 0)
	{
		<div class="path">Product</div>

		<div class="content-main">
			
			@foreach (var item in Model.Products)
			{
				<div class="product">
					<a href="~/Product/Detail?id=@item.ProductId">
						@if (@item.CategoryId == 1)
						{
							<img style="width:150px;" src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcTSdq9LeaalIOGwghpv1PsJrbVw4NmL2uo2ebpPy828JwLiGtimHwJpBx0HxOveknulsyY&usqp=CAU" alt="Category 1 Image" />
						}
						else if (@item.CategoryId == 2)
						{
							<img style="width:150px;" src="data:image/jpeg;base64," alt="Category 2 Image" />
						}
						else if (@item.CategoryId == 3)
						{
							<img style="width:150px;" src="data:image/jpeg;base64," />

						}	
						else if (@item.CategoryId == 4)
						{
							<img style="width:150px;" src="data:image/jpeg;base64," />

						}
						else if (@item.CategoryId == 5)
						{
							<img style="width:150px;" src="data:image/jpeg;base64," />

						}
						else if (@item.CategoryId == 6)
						{
							<img style="width:150px;" src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcSU4Hgmu3FlBpEPW-Cylvwo4rFfFvycVhwt3w&usqp=CAU" />

						}
					</a>
					<div class="name"><a href="~/Product/Detail?id=@item.ProductId">@item.ProductName</a></div>
					<div class="price">@item.UnitPrice$</div>
					<div class="buynow"><a href="">Buy now</a></div>
				</div>	
			}
		</div>
	}
	else
	{
		<div class="path">Best Sale</div>

		<div class="content-main">
			@foreach (var item in Model.BestSaleProducts)
			{
				<div class="product">
					<a asp-page="./Product/Detail" asp-route-id="@item.ProductId"><img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcTSdq9LeaalIOGwghpv1PsJrbVw4NmL2uo2ebpPy828JwLiGtimHwJpBx0HxOveknulsyY&usqp=CAU" /></a>
					<div class="name"><a asp-page="./Product/Detail" asp-route-id="@item.ProductId">@item.ProductName</a></div>
					<div class="price">@item.UnitPrice$</div>
					<div><a href="">Buy now</a></div>
				</div>
			}
		</div>

		<div class="path">New Product</div>

		<div class="content-main">
			@foreach (var item in Model.NewProducts)
			{
				<div class="product">
					<a asp-page="./Product/Detail" asp-route-id="@item.ProductId"><img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcTSdq9LeaalIOGwghpv1PsJrbVw4NmL2uo2ebpPy828JwLiGtimHwJpBx0HxOveknulsyY&usqp=CAU" /></a>
					<div class="name"><a asp-page="./Product/Detail" asp-route-id="@item.ProductId">@item.ProductName</a></div>
					<div class="price">@item.UnitPrice$</div>
					<div><a href="">Buy now</a></div>
				</div>
			}
		</div>
	}

</div>